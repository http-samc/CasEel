{
    "manifest_version": 2,
    "name": "CasEel",
    "version": "1.0",
    "description": "A Chrome Extension for autosaving NetMath notebooks!",
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "assets/images/16x16.png",
            "32": "assets/images/32x32.png",
            "192": "assets/images/192x192.png"
        }
    },
    "permissions": [
        "tabs",
        "http://courseware.illinois.edu/*",
        "https://courseware.illinois.edu/*"
    ],
    "background": {
        "scripts": [
            "assets/js/background.js"
        ],
        "persistent": false
    }
}